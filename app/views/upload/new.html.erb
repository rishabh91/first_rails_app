<% provide(:title,"File Upload") %>
<div class="row">
<div class = "col-md-6 col-md-offset-3 ">
   <%= form_for :upload, url: uploadsubmit_path do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :attachment %>
      <%= f.file_field :attachment %>
      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>
</div>
</div>

